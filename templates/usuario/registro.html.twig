{% extends 'layouts/master.html.twig' %}


 {% block titulo %}Registrar usuario{% endblock %}

{% block form %}

    <div class="container">
        <div>
            <h5 class="center-align">Formulario de Usuarios</h5>
        </div>
    </div>

    </br>
    
        {% for message in app.session.flashbag().get('message') %}

            <center><strong>{{ message }}</strong></center>

        {% endfor %}
    
    <div class="container">
        <div class="row">

            <div class="row card-panel">
                
                {{ form_start(form) }}
                {{ form_widget(form) }}
                {{ form_end(form) }}

            </div> 

        </div> 

    </div>



{% endblock %}

